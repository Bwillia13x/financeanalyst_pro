# Production Environment Configuration
# FinanceAnalyst Pro - Production Settings

# Application Environment
VITE_APP_ENV=production
VITE_APP_VERSION=1.0.0

# API Configuration
# Replace with your production API keys
VITE_ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here
VITE_FMP_API_KEY=your_fmp_api_key_here
VITE_POLYGON_API_KEY=your_polygon_api_key_here

# API Endpoints (Production)
VITE_ALPHA_VANTAGE_BASE_URL=https://www.alphavantage.co/query
VITE_FMP_BASE_URL=https://financialmodelingprep.com/api/v3
VITE_POLYGON_BASE_URL=https://api.polygon.io/v2

# Performance & Caching
VITE_CACHE_ENABLED=true
VITE_CACHE_TTL=300000
VITE_RATE_LIMITING_ENABLED=true
VITE_MAX_REQUESTS_PER_MINUTE=60

# Security Settings
VITE_FORCE_DEMO_MODE=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# Monitoring & Analytics
VITE_SENTRY_DSN=your_sentry_dsn_here
VITE_GA_TRACKING_ID=your_google_analytics_id_here
VITE_HOTJAR_ID=your_hotjar_id_here

# Feature Flags
VITE_ENABLE_ADVANCED_FEATURES=true
VITE_ENABLE_BETA_FEATURES=false
VITE_ENABLE_DEBUG_MODE=false

# CDN & Assets
VITE_CDN_URL=https://cdn.yourapp.com
VITE_ASSETS_BASE_URL=/assets/

# Application URLs
VITE_APP_URL=https://financeanalyst.pro
VITE_API_BASE_URL=https://api.financeanalyst.pro
VITE_SUPPORT_URL=https://support.financeanalyst.pro

# Database & Storage (if applicable)
VITE_STORAGE_BUCKET=financeanalyst-prod-storage
VITE_BACKUP_ENABLED=true

# Compliance & Legal
VITE_PRIVACY_POLICY_URL=https://financeanalyst.pro/privacy
VITE_TERMS_OF_SERVICE_URL=https://financeanalyst.pro/terms
VITE_COOKIE_CONSENT_ENABLED=true

# Performance Monitoring
VITE_PERFORMANCE_MONITORING=true
VITE_LIGHTHOUSE_CI=true
VITE_CORE_WEB_VITALS_TRACKING=true
