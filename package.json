{
  "name": "financeanalyst-pro",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "dependencies": {
    "@dhiwise/component-tagger": "^1.0.10",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
