{
  "name": "financeanalyst-pro",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@reduxjs/toolkit": "^2.6.1",
    "@sentry/browser": "^9.39.0",
    "@sentry/react": "^10.5.0",
    "axios": "^1.8.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "exceljs": "^4.4.0",
    "framer-motion": "^10.16.4",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.484.0",
    "openai": "^5.12.2",
    "pptxgenjs": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.55.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.26.0",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "recharts": "^2.15.4",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^5.0.3"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build --sourcemap",
    "serve": "vite preview",
    "preview": "vite preview --port 4173",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run src/test/unit/ --coverage",
    "test:ui:components": "vitest -c vitest.ui.config.mjs",
    "test:onboarding": "vitest run src/hooks/__tests__/useOnboarding.test.jsx",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "test:smoke": "playwright test --grep @smoke",
    "e2e:install": "playwright install --with-deps",
    "e2e:dev": "PW_USE_DEV_SERVER=1 playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:chromium": "playwright test --project=chromium",
    "lint": "eslint src --ext .js,.jsx --report-unused-disable-directives",
    "lint:strict": "eslint src --ext .js,.jsx --report-unused-disable-directives --max-warnings 0 --no-error-on-unmatched-pattern",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,css,md}\"",
    "type-check": "echo 'No TypeScript configured'",
    "lighthouse": "lighthouse http://localhost:5173 --output html --output-path lighthouse-report.html",
    "lighthouse:ci": "lighthouse http://localhost:5173 --output json --output-path lighthouse-results.json",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/static/js/*.js",
    "build:prod": "NODE_ENV=production npm run build",
    "build:staging": "NODE_ENV=staging npm run build",
    "build:preview": "npm run build && npm run serve",
    "deploy:netlify": "npm run build:prod && netlify deploy --prod --dir dist",
    "deploy:vercel": "npm run build:prod && vercel --prod",
    "deploy:gh-pages": "npm run build:prod && gh-pages -d dist",
    "size-check": "npm run build && npx bundlesize",
    "perf-check": "npm run build && lighthouse http://localhost:4173 --output json --output-path perf-results.json",
    "ci:build": "npm ci && npm run lint && npm run test:run && npm run build:prod",
    "ci:deploy": "npm run ci:build && npm run deploy:netlify",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:install": "npm run clean && npm install",
    "predeploy": "npm run lint && npm run test:run && npm run build:prod",
    "analyze": "npm run build && npx vite-bundle-analyzer",
    "budget:check": "node scripts/check-bundle-budget.cjs",
    "perf:audit": "npm run build && npm run lighthouse:ci && npm run budget:check",
    "perf:dev": "npm run build && npm run budget:check",
    "analyze:bundle": "npm run build && node scripts/analyze-bundle.mjs",
    "optimize": "npm run analyze:bundle && npm run budget:check",
    "staging:deploy": "node scripts/staging-deploy.cjs",
    "staging:audit": "npm run build && npm run staging:deploy",
    "quality:check": "npm run lint && npm run format:check && npm audit --audit-level=moderate",
    "quality:fix": "npm run lint:fix && npm run format",
    "pre-deploy": "npm run quality:check && npm run test:run && npm run build",
    "ci:quality": "npm run quality:check && npm run test:coverage",
    "lint:all": "npm run lint && cd backend && npm run lint && cd ..",
    "lint:fix:all": "npm run lint:fix && cd backend && npm run lint && cd ..",
    "quality:check:all": "npm run lint:all && npm run format:check && npm audit --audit-level=moderate",
    "quality:fix:all": "npm run lint:fix:all && npm run format",
    "install:all": "npm install && cd backend && npm install && cd ..",
    "test:all": "npm run test:run && cd backend && npm run test && cd ..",
    "test:comprehensive": "node src/test/TestRunner.js",
    "test:integration": "vitest run src/test/integration/ --coverage",
    "test:e2e": "playwright test",
    "test:performance": "k6 run src/test/performance/load-test.js",
    "test:security": "npm audit && node src/test/security/security-scan.js",
    "test:mobile": "playwright test src/test/mobile/ --project=mobile",
    "test:accessibility": "playwright test src/test/accessibility/ --grep accessibility",
    "test:critical": "playwright test --grep @critical",
    "test:ci": "npm run test:unit && npm run test:integration && npm run test:smoke",
    "test:backend": "cd backend && npm test",
    "e2e:ci": "playwright test --project=chromium",
    "test:full-suite": "npm run test:comprehensive && npm run test:e2e && npm run test:performance",
    "test:browser": "echo 'Open browser console and run: FinanceAnalystTestRunner.runAllTests()'",
    "test:debug": "vitest --inspect-brk",
    "test:watch": "vitest --watch",
    "test:coverage:full": "vitest run --coverage --coverage.reporter=json,html,lcov",
    "test:parallel": "vitest run --shard=1/2",
    "test:flaky": "vitest run --retry=3 --reporter=verbose",
    "quality:audit": "npm run lint && npm run test:security && npm run test:accessibility",
    "build:all": "npm run build",
    "dev:full": "concurrently \"npm start\" \"cd backend && npm run dev\"",
    "setup:dev": "npm run install:all && echo 'Development environment ready!'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@axe-core/react": "^4.10.2",
    "@eslint/js": "^9.31.0",
    "@playwright/test": "^1.47.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/line-clamp": "^0.1.0",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.3.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autocannon": "^8.0.0",
    "autoprefixer": "10.4.2",
    "axe-core": "^4.10.3",
    "concurrently": "^8.2.2",
    "cypress": "^14.5.4",
    "cypress-real-events": "^1.14.0",
    "eslint": "^9.33.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jest": "^30.1.1",
    "jest-axe": "^10.0.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^30.1.1",
    "jsdom": "^26.1.0",
    "lighthouse": "^12.8.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "puppeteer": "^24.17.1",
    "tailwindcss": "3.4.6",
    "terser": "^5.43.1",
    "typescript": "^5.9.2",
    "vite": "^6.0.4",
    "vite-tsconfig-paths": "3.6.0",
    "vitest": "^3.2.4"
  }
}
