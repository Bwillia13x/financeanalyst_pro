    "@radix-ui/react-slot": "^1.2.3",
    "@reduxjs/toolkit": "^2.6.1",
    "@sentry/browser": "^9.39.0",
    "@sentry/react": "^10.5.0",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^1.8.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.0.1",
    "framer-motion": "^10.16.4",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "jszip": "^3.10.1",
    "lucide-react": "^0.484.0",
    "openai": "^5.12.2",
    "papaparse": "^5.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.55.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "6.0.2",
    "react-router-hash-link": "^2.4.3",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "recharts": "^2.15.4",
    "redux": "^5.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-elevation": "^2.0.0",
    "tailwindcss-fluid-type": "^2.0.7",
    "web-vitals": "^5.0.3",
    "xlsx": "^0.18.5"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build --sourcemap",
    "serve": "vite preview",
    "preview": "vite preview --port 4173",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest -c vitest.unit.config.mjs",
    "test:ui:components": "vitest -c vitest.ui.config.mjs",
    "test:onboarding": "vitest run src/hooks/__tests__/useOnboarding.test.jsx",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "test:smoke": "playwright test -g @smoke",
    "e2e:install": "playwright install --with-deps",
    "e2e:dev": "PW_USE_DEV_SERVER=1 playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:chromium": "playwright test --project=chromium",
    "lint": "eslint src --ext .js,.jsx --report-unused-disable-directives",
    "lint:strict": "eslint src --ext .js,.jsx --report-unused-disable-directives --max-warnings 0 --no-error-on-unmatched-pattern",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,css,md}\"",
    "type-check": "echo 'No TypeScript configured'",
    "lighthouse": "lhci autorun --config lighthouse.config.cjs",
    "lighthouse:ci": "lhci autorun --config lighthouse.config.cjs",
    "analyze": "npm run build && npx vite-bundle-analyzer",
    "budget:check": "node scripts/check-bundle-budget.cjs",
    "perf:audit": "npm run build && npm run lighthouse:ci && npm run budget:check",
    "perf:dev": "npm run build && npm run budget:check",
    "analyze:bundle": "npm run build && node scripts/analyze-bundle.mjs",
    "optimize": "npm run analyze:bundle && npm run budget:check",
    "staging:deploy": "node scripts/staging-deploy.cjs",
    "staging:audit": "npm run build && npm run staging:deploy",
    "quality:check": "npm run lint && npm run format:check && npm audit --audit-level=moderate",
    "quality:fix": "npm run lint:fix && npm run format",
    "pre-deploy": "npm run quality:check && npm run test:run && npm run build",
    "ci:quality": "npm run quality:check && npm run test:coverage",
    "lint:all": "npm run lint && cd backend && npm run lint && cd ..",
    "lint:fix:all": "npm run lint:fix && cd backend && npm run lint && cd ..",
    "quality:check:all": "npm run lint:all && npm run format:check && npm audit --audit-level=moderate",
    "quality:fix:all": "npm run lint:fix:all && npm run format",
    "install:all": "npm install && cd backend && npm install && cd ..",
    "test:all": "npm run test:run && cd backend && npm run test && cd ..",
    "build:all": "npm run build",
    "dev:full": "concurrently \"npm start\" \"cd backend && npm run dev\"",
    "setup:dev": "npm run install:all && echo 'Development environment ready!'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@axe-core/react": "^4.10.2",
    "@eslint/js": "^9.31.0",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.47.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/line-clamp": "^0.1.0",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "10.4.2",
    "axe-core": "^4.10.3",
    "eslint": "^9.33.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "3.4.6",
    "typescript": "^5.9.2",
    "vite": "^6.0.4",
    "vite-tsconfig-paths": "3.6.0",
    "vitest": "^3.2.4",
    "concurrently": "^8.2.2"
  }
}
