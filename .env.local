# Staging Environment Configuration
# FinanceAnalyst Pro - Staging Settings

# Application Environment
VITE_APP_ENV=staging
VITE_APP_VERSION=1.0.0-staging

# API Configuration
# Use staging/test API keys
VITE_ALPHA_VANTAGE_API_KEY=your_staging_alpha_vantage_api_key_here
VITE_FMP_API_KEY=your_staging_fmp_api_key_here
VITE_POLYGON_API_KEY=your_staging_polygon_api_key_here

# API Endpoints (Staging)
VITE_ALPHA_VANTAGE_BASE_URL=https://www.alphavantage.co/query
VITE_FMP_BASE_URL=https://financialmodelingprep.com/api/v3
VITE_POLYGON_BASE_URL=https://api.polygon.io/v2

# Performance & Caching (More relaxed for testing)
VITE_CACHE_ENABLED=true
VITE_CACHE_TTL=60000
VITE_RATE_LIMITING_ENABLED=true
VITE_MAX_REQUESTS_PER_MINUTE=120

# Security Settings
VITE_FORCE_DEMO_MODE=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# Monitoring & Analytics (Staging)
VITE_SENTRY_DSN=your_staging_sentry_dsn_here
VITE_GA_TRACKING_ID=your_staging_google_analytics_id_here
VITE_HOTJAR_ID=your_staging_hotjar_id_here

# Feature Flags (More permissive for testing)
VITE_ENABLE_ADVANCED_FEATURES=true
VITE_ENABLE_BETA_FEATURES=true
VITE_ENABLE_DEBUG_MODE=true

# CDN & Assets
VITE_CDN_URL=https://staging-cdn.yourapp.com
VITE_ASSETS_BASE_URL=/assets/

# Application URLs
VITE_APP_URL=https://staging.financeanalyst.pro
VITE_API_BASE_URL=https://staging-api.financeanalyst.pro
VITE_SUPPORT_URL=https://support.financeanalyst.pro

# Database & Storage (Staging)
VITE_STORAGE_BUCKET=financeanalyst-staging-storage
VITE_BACKUP_ENABLED=false

# Compliance & Legal
VITE_PRIVACY_POLICY_URL=https://staging.financeanalyst.pro/privacy
VITE_TERMS_OF_SERVICE_URL=https://staging.financeanalyst.pro/terms
VITE_COOKIE_CONSENT_ENABLED=false

# Performance Monitoring
VITE_PERFORMANCE_MONITORING=true
VITE_LIGHTHOUSE_CI=true
VITE_CORE_WEB_VITALS_TRACKING=true

# Testing & Development
VITE_ENABLE_TEST_DATA=true
VITE_MOCK_API_RESPONSES=false
VITE_VERBOSE_LOGGING=true
