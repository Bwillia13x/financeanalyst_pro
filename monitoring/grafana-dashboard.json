{
  "dashboard": {
    "id": null,
    "title": "FinanceAnalyst Pro - Production Monitoring",
    "tags": ["financeanalyst", "production", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System Health Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"financeanalyst-pro\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Application Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"financeanalyst-pro\"}[5m]))",
            "legendFormat": "95th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"financeanalyst-pro\"}[5m]))",
            "legendFormat": "50th percentile",
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\", job=\"financeanalyst-pro\"}[5m]) / rate(http_requests_total{job=\"financeanalyst-pro\"}[5m]) * 100",
            "legendFormat": "Error rate %",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(cpu_usage_seconds_total{job=\"financeanalyst-pro\"}[5m]) * 100",
            "legendFormat": "CPU Usage %",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"financeanalyst-pro\"} / 1024 / 1024",
            "legendFormat": "Memory Usage (MB)",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Active Users",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(active_users{job=\"financeanalyst-pro\"})",
            "legendFormat": "Active Users",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "API Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"financeanalyst-pro\"}[5m])",
            "legendFormat": "Requests/second",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 8,
        "title": "Database Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "pg_stat_activity_count{datname=\"financeanalyst\"}",
            "legendFormat": "Active connections",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      },
      {
        "id": 9,
        "title": "Security Events",
        "type": "table",
        "targets": [
          {
            "expr": "security_events_total{job=\"financeanalyst-pro\"}",
            "legendFormat": "{{ event_type }}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 10,
        "title": "Performance Alerts",
        "type": "alertlist",
        "alertName": "FinanceAnalyst Performance Alerts",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 40
        }
      },
      {
        "id": 11,
        "title": "Business Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(financial_analysis_requests_total[5m]))",
            "legendFormat": "Analysis Requests",
            "refId": "A"
          },
          {
            "expr": "sum(rate(api_calls_total{endpoint=~\"/market.*\"}[5m]))",
            "legendFormat": "Market Data Requests",
            "refId": "B"
          },
          {
            "expr": "sum(rate(portfolio_analysis_total[5m]))",
            "legendFormat": "Portfolio Analysis",
            "refId": "C"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 40
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}
